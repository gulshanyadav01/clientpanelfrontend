:[],\n    client:{}\n}\n\nconst ClientReducer = (state = initialState, action) => { \n    switch(action.type){\n        \n        case ADD_CLIENT:\n            return{\n                ...state,\n                clients: [...state.clients, action.payload]\n                \n        }\n        case GET_CLIENTS:\n            return{\n                ...state,\n                clients:action.payload\n            }\n        case DELETE_CLIENT_BY_ID:\n            return{\n                ...state,\n                clients: state.clients.filter(client =>{\n                    return(\n                        client.id !== action.payload\n                    )\n                })\n            }\n        case GET_CLIENT_BY_ID:\n            return{\n                ...state,\n                client:action.payload\n            }\n        default:\n            return state;\n    }\n}\n\nexport default ClientReducer; ","/home/gulshan/Desktop/Project/clientpanelfrontend/src/components/Clients.js",[],"/home/gulshan/Desktop/Project/clientpanelfrontend/src/components/AppNavbar.js",[],"/home/gulshan/Desktop/Project/clientpanelfrontend/src/components/Sidebar.js",[],"/home/gulshan/Desktop/Project/clientpanelfrontend/src/components/Detail.js",[],"/home/gulshan/Desktop/Project/clientpanelfrontend/src/components/UserAuth/Register.js",["89"],"import React, { useState } from 'react'\nimport {connect} from \"react-redux\";\nimport {registerUser} from \"../../Store/Action/AuthAction\"\nimport {Redirect, useHistory} from \"react-router-dom\";\n\n\nconst Register = ({registerUser}) => {\n    const history = useHistory();\n    const [formdata, setFormData] = useState({\n        name:\"\",\n        email:\"\",\n        password:\"\"\n    })\n\n    const onChange = (e) => {\n        setFormData({...formdata, [e.target.name]: e.target.value});\n\n    \n    }\n\n    const onSubmit = (e) => {\n        e.preventDefault();\n        const {name, email, password} = formdata;\n        console.log(`${name